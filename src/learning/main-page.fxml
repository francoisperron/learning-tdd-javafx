<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.192" xmlns:fx="http://javafx.com/fxml/1"
      prefHeight="600.0"
      prefWidth="640.0">
    <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="100.0">
        <TextField fx:id="newTodo"/>
        <Button fx:id="addButton" contentDisplay="CENTER" mnemonicParsing="false" text="Add"/>
    </HBox>
    <HBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="100.0">
        <Label fx:id="todos"/>
    </HBox>
</VBox>
